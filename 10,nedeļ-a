# 1. un 2. uzd.
lenght = int(input ("Lenght: "))
height = int(input ("Height: "))
for i in range(0, lenght):
    for j in range(0, height):
        print("*", end = " ")
    print()
    
#3. uzd

length = int(input("Lenght: "))
for i in range(1, length + 1):
  print(" " * (length - i), end="")
  print("* " * i)
